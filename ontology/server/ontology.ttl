                   rdfs:subClassOf <http://w3id.org/devops-infra/core#Resource> ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :flavor ;
                                     owl:allValuesFrom :HostFlavor
                                   ] ;
                   rdfs:label "Host Configuration"@en .


