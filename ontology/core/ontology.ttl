###  http://w3id.org/devops-infra/core#createTime
:createTime rdf:type owl:DatatypeProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :Resource
                                        :ResourceGroup
                                      )
                        ] ;
            rdfs:range xsd:dateTime ;
            rdfs:comment "Date and time when a Resource or Resource Group is created"@en ;
            rdfs:label "create time"@en .


###  http://w3id.org/devops-infra/core#updateTime
:updateTime rdf:type owl:DatatypeProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :Resource
                                        :ResourceGroup
                                      )
                        ] ;
            rdfs:range xsd:dateTime ;
            rdfs:comment "The date and time when a Resource or Resource Group is updated"@en ;
            rdfs:label "update time"@en .


###  http://w3id.org/devops-infra/core#version
:version rdf:type owl:DatatypeProperty ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( :Resource
                                     :ResourceGroup
                                   )
                     ] ;
         rdfs:range xsd:string ;
         rdfs:comment "This property allows representing the version of any entity (Resource or ResourceGroup)"@en ;
         rdfs:label "version"@en .


