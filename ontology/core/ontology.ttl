###  http://w3id.org/devops-infra/core#Resource
:Resource rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :belongsToResourceGroup ;
                            owl:allValuesFrom :ResourceGroup
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :createTime ;
                            owl:allValuesFrom xsd:dateTime
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :updateTime ;
                            owl:allValuesFrom xsd:dateTime
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :version ;
                            owl:allValuesFrom xsd:string
                          ] ;
          rdfs:comment "A Resource is any relevant entity that is managed in the context of a DevOps infrastructure in this ontology network"@en ;
          rdfs:label "Resource"@en .


###  http://w3id.org/devops-infra/core#ResourceGroup
:ResourceGroup rdf:type owl:Class ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty :parentResourceGroup ;
                                 owl:allValuesFrom :ResourceGroup
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :createTime ;
                                 owl:allValuesFrom xsd:dateTime
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :updateTime ;
                                 owl:allValuesFrom xsd:dateTime
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :version ;
                                 owl:allValuesFrom xsd:string
                               ] ;
               rdfs:comment "A Resource Group identifies a grouping of entities (resources) that are managed in the context of a DevOps infrastructure in this ontology network"@en ;
               rdfs:label "Resource Group"@en .


